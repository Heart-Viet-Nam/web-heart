{
  "name": "heart-web",
  "version": "1.0.0",
  "description": "Heart web ",
  "main": "src/server.ts",
  "scripts": {
    "minify": "rimraf ./public/js & rimraf ./public/css & rimraf ./public/img && gulp minify-files",
    "minify:watch": "gulp watch-files",
    "dev": "cross-env NODE_ENV=development npm run minify && ts-node-dev src/server.ts",
    "start": "cross-env NODE_ENV=production tsc && node dist/server.js",
    "build": "rimraf ./dist && npx tsc -b",
    "deploy": "npm run minify && npm run build && rimraf ./build && webpack && rimraf ./dist",
    "check": "prettier --check \"src/**/*.ts\" ",
    "format": "prettier --write \"src/**/*.ts\" "
  },
